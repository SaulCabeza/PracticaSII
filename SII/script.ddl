CREATE TABLE ORDENES_PAGO (NUMERO DECIMAL(15) NOT NULL, EMISOR VARCHAR(255), FECHA DATE, PRIMARY KEY (NUMERO))
CREATE TABLE INGRESOS (CODIGO_INGRESO DECIMAL(15) NOT NULL, CANTIDAD BIGINT, DESCRIPCION VARCHAR(255), FECHA DATE, PROYECTOS_CÓDIGO DECIMAL(15), PRIMARY KEY (CODIGO_INGRESO))
CREATE TABLE PROYECTOS (CÓDIGO DECIMAL(15) NOT NULL, COSTE_TOTAL BIGINT, DESCRIPCIÓN VARCHAR(255), NOMBRE_PROYECTO VARCHAR(255), REPARTO_COMBUSTIBLE BIGINT, REPARTO_CONTENEDOR BIGINT, REPARTO_MANTENIMIENTO BIGINT, PRIMARY KEY (CÓDIGO))
CREATE TABLE EGRESOS (CODIGO_EGRESO DECIMAL(15) NOT NULL, CONCEPTO VARCHAR(255), FECHA DATE, IMPORTE BIGINT, NOMBRE_BENEFICIARIO VARCHAR(255), ORDENES_PAGO_NUMERO DECIMAL(15), PROYECTOS_CÓDIGO DECIMAL(15), PRIMARY KEY (CODIGO_EGRESO))
ALTER TABLE INGRESOS ADD CONSTRAINT NGRSSPRYCTOSCÓDIGO FOREIGN KEY (PROYECTOS_CÓDIGO) REFERENCES PROYECTOS (CÓDIGO)
ALTER TABLE EGRESOS ADD CONSTRAINT GRSOSPRYCTOSCÓDIGO FOREIGN KEY (PROYECTOS_CÓDIGO) REFERENCES PROYECTOS (CÓDIGO)
ALTER TABLE EGRESOS ADD CONSTRAINT GRSSRDNSPAGONUMERO FOREIGN KEY (ORDENES_PAGO_NUMERO) REFERENCES ORDENES_PAGO (NUMERO)
